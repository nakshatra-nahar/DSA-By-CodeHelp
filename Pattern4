#include <iostream>
using namespace std;
int main() {
    int size;
    cout<<"enter A Size Of Pattern :- ";
    cin>>size;
    int row = 1;
    while(row<=size){
        int col = size;
        while(col>=1){
            cout<<col<<" ";
            col--;
        }
        cout<<endl;
        row++;
    }

    return 0;
}
