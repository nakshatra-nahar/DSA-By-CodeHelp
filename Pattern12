#include <iostream>
using namespace std;

int main() {
    int size;
    cout<<"enter A Size Of Pattern :- ";
    cin>>size;
    int i =0;
    while(i<size){
        int j =1;
        while(j<=size){
            cout<<char(int('A')+i)<<" ";
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}
