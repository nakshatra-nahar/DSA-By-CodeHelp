#include <iostream>
using namespace std;

int main() {
    int size;
    cout<<"Enter Size :- ";
    cin>>size;
    int i = 1;
    while(i<=size){
        int j = i;
        while(j<size+i){
            cout<<char(int('A')+j-1)<<" ";
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}  
