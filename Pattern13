#include <iostream>
using namespace std;

int main() {
    int size;
    cout<<"Enter Size :- ";
    cin>>size;
    int count = 0;
    int i =1;
    while(i<=size){
        int j =1;
        while(j<=size){
            cout<<char(int('A')+count)<<" ";
            count++;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}
