#include <iostream>
using namespace std;
int main() {
    int size;
    int row = 1;
  
    cout<<"Enter Size :- ";
    cin>>size;
    while(row<=size){
        int col = 1;
        while(col<=size){
            cout<<"*"<<" ";
            col++;
    }
    cout<<endl;
    row++;
  }

    return 0;
}
