#include <iostream>
using namespace std;
int main() {
    int size;
    cout<<"enter A Size Of Pattern :- ";
    cin>>size;
    int count = 1;
    int row = 1;
    while(row<=size){
        int col = 1;
        while(col<=size){
            cout<<count<<" ";
            count++;
            col++;
        }
        cout<<endl;
        row++;
    }
    return 0;
}
