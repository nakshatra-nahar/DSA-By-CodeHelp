#include <iostream>
using namespace std;

int main() {
    int size;
    cout<<"enter A Size Of Pattern :- ";
    cin>>size;
    int i = 1;
    while(i<=size){
        int j = 0;
        while(j<size){
            cout<<char(int('A')+j)<<" ";
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}
