#include <iostream>
using namespace std;

int main() {
    int size;
    cout<<"Enter Size :- ";
    cin>>size;
    int count =1;
    int i=1;
    while(i<=size){
        int j = 1;
        while(j<=i){
            cout<<char(int('A')+count-1)<<" ";
            count++;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}  
