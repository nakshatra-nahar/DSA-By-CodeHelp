#include <iostream>
using namespace std;

int main() {
    int size;
    cout<<"enter A Size Of Pattern :- ";
    cin>>size;
    int i = 1;
    int count = 1;
    while(i<=size){
        int j = 1;
        while(j<=i){
            cout<<count<<" ";
            count++;
            j++;
        }
        cout<<endl;
        i++;
    }
    return 0;
}
