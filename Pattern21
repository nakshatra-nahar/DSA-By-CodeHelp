#include <iostream>
using namespace std;
int main() {
    int size;
    cout<<"Enter Size :- ";
    cin>>size;
    for(int i=1;i<=size;i++){
        for(int j=1;j<i;j++){
            cout<<" ";
        }
        for(int k=size;k>=i;k--){
            cout<<i;   
        }
        cout<<endl;
    }
    return 0;
}
