#include <iostream>
using namespace std;

int main() {
    int size;
    cout<<"enter A Size Of Pattern :- ";
    cin>>size;
    int i = 1;
    while(i<=size){
        int j = i;
        while(j>=1){
            cout<<j<<" ";
            j--;
        }
        cout<<endl;
        i++;
    }
    return 0;
}
